{
  "name": "@dxkit-org/react-js-kit",
  "version": "1.0.0",
  "description": "Modern TypeScript React utility library with tree-shaking support - Collection of React-specific utilities and Zustand helpers for TypeScript projects",
  "keywords": [
    "typescript",
    "react",
    "utilities",
    "utility-library",
    "tree-shaking",
    "esm",
    "zustand",
    "state-management",
    "react-hooks",
    "selectors",
    "modern-js",
    "typescript-library",
    "react-kit",
    "functional",
    "performance",
    "environment-detection",
    "cross-platform"
  ],
  "type": "module",
  "main": "./dist/react/index.js",
  "module": "./dist/react/index.js",
  "react-native": "./dist/react/index.js",
  "types": "./dist/react/index.d.ts",
  "typings": "./dist/react/index.d.ts",
  "typesVersions": {
    "*": {
      "zustand": [
        "./dist/react/zustand/index.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/react/index.d.ts",
      "react-native": "./dist/react/index.js",
      "module": "./dist/react/index.js",
      "import": "./dist/react/index.js",
      "default": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./zustand": {
      "types": "./dist/react/zustand/index.d.ts",
      "react-native": "./dist/react/zustand/index.js",
      "module": "./dist/react/zustand/index.js",
      "import": "./dist/react/zustand/index.js",
      "default": "./dist/react/zustand/index.js",
      "require": "./dist/react/zustand/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dxkit-org/react-js-kit.git"
  },
  "homepage": "https://github.com/dxkit-org/react-js-kit#readme",
  "bugs": {
    "url": "https://github.com/dxkit-org/react-js-kit/issues"
  },
  "author": {
    "name": "Adarsh Hatkar",
    "url": "https://github.com/AdarshHatkar"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "npx rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release:patch": "npm version patch",
    "postversion": "git push && git push --tags && npm publish",
    "link": "npm run build && npm link",
    "unlink": "npm unlink -g @dxkit-org/react-js-kit"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "zustand": "^5.0.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
